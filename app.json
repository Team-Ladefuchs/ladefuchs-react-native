{
  "expo": {
    "name": "Ladefuchs",
    "slug": "ladefuchs",
    "version": "2.1.0",
    "orientation": "portrait",
    "scheme": "com.ladefuchs.app",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ladefuchs.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3EEE2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "14",
      "supportsTablet": false,
      "jsEngine": "jsc",
      "bundleIdentifier": "app.ladefuchs.Ladefuchs",
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "versionCode": 218,
      "adaptiveIcon": {
        "foregroundImage": "./assets/android_logo.png",
        "backgroundColor": "#F3EEE2"
      },
      "package": "app.ladefuchs.android"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ],
      [
        "expo-native-fonts",
        {
          "Roboto": [
            {
              "path": "./assets/fonts/Roboto-Bold.ttf"
            }
          ],
          "Bitter": [{ "path": "./assets/fonts/Bitter-Italic.ttf" }],
          "RobotoCondensed": [
            { "path": "./assets/fonts/Roboto-Condensed-Bold.ttf" }
          ]
        }
      ]
    ]
  }
}
